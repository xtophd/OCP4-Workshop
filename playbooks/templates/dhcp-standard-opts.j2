#
# This configuration file is concatination of fragments 
# generated by an ansible playbook
#   /etc/dhcp/cfg-fragments
#

default-lease-time 7200;
max-lease-time 21600;

{% if g_usePXE == 'True' %}
option client-system-arch code 93 = unsigned integer 16;
{% endif %}

{% if g_useUEFI_HTTP == 'True' %}
## Variable is used to aid specification of UEFI HTTP method
set uefi_http_iso = "false";
{% endif %}

